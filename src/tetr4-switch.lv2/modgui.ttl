@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.ipngn/ns/lv2core#> .

<http://srmourasilva.github.io/plugins/tetr4-switch>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-.html> ;
        modgui:stylesheet
            <modgui/css/dip-switch.css> ,
            <modgui/css/pedal-base.css> ,
            <modgui/css/pedal-boxy.css>
            ;
        modgui:javascript <modgui/js/js.js> ;
        modgui:screenshot <modgui/screenshot-.png> ;
        modgui:thumbnail <modgui/thumbnail-.png> ;
        modgui:brand "SrMouraSilva" ;
        modgui:label "Tetr4 switch" ;
        modgui:model "boxy-small" ;
        modgui:panel "1-footswitch" ;
        modgui:color "black" ;
    ] .